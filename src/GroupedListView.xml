<?xml version="1.0" encoding="utf-8"?>
<widget id="conclusion low code company.groupedlistview.GroupedListView" pluginWidget="true" needsEntityContext="true" offlineCapable="true"
        supportedPlatform="Web"
        xmlns="http://www.mendix.com/widget/1.0/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.mendix.com/widget/1.0/ ../node_modules/mendix/custom_widget.xsd">
    <name>Grouped List View</name>
    <description>List view with grouped items.</description>
    <icon>
		
	</icon>
    <properties>
        <propertyGroup caption="Source">
            <property key="inputList" type="datasource" isList="true" required="true">
                <caption>Data source</caption>
                <description>A list of objects, at least sorted on the attribute that identifies a group.</description>
            </property>
            <property key="groupId" type="attribute" dataSource="inputList" required="true">
                <caption>Group identifier</caption>
                <description>The attribute that identifies a group.</description>
                <attributeTypes>
                    <attributeType name="String" />
                </attributeTypes>
            </property>
            <property key="groupTitle" type="widgets" required="true" dataSource="inputList">
                <caption>Group title (source is the first object for this group)</caption>
                <description>Widgets using data source</description>
            </property>
            <property key="widget" type="widgets" required="true" dataSource="inputList">
                <caption>Content</caption>
                <description>Widgets using data source</description>
            </property>
            <property key="emptyMessage" type="widgets" required="false">
                <caption>Empty message</caption>
                <description></description>
            </property>
            <property key="stickyGroupHeaders" type="boolean" defaultValue="false">
                <caption>Sticky Group Headers</caption>
                <description>While scrolling, the group header will stay visible.</description>
            </property>
        </propertyGroup>
        <propertyGroup caption="Events">
            <property key="listItemClick" type="action" dataSource="inputList">
                <caption>On click</caption>
                <description></description>
            </property>
        </propertyGroup>
    </properties>
</widget>